<?xml version="1.0" encoding="UTF-8"?>
<root>
	<!-- Sezione dedicata ai default di progetto -->
	<projectSection>
		<skinProperties>
			<styles>
				<global>		
					<ProjectBoxattrStyle 
					backgroundColor="0x999999"
					backgroundAlpha="1"	
					foregroundColor="0x333333"
					paddingTop="5"
					paddingBottom="5"
					paddingLeft="5"
					paddingRight="5"
					borderStyle="solid"
					borderRadius="10"
					shadow="true"
					shadowColor="0x666666"
					borderSize="1"
					borderColor="0x666666"
				/>
					<attrLabelStyle 
					backgroundColor="0xcccccc"	
					foregroundColor="0x000000"
				/>
					<attrInputTextStyle 
					backgroundColor="0xffffff"	
					backgroundAlpha="1"
					foregroundColor="0x333333"
					marginTop="5"
					marginBottom="5"
					marginLeft="5"
					marginRight="5"
					borderStyle="solid"
					borderSize="1"
					borderColor="0x666666"
				/>
					<ProjectBoxItemStyle 
					backgroundColor="0xcccccc"	
					backgroundAlpha="1"
					foregroundColor="0x333333"
					marginTop="5"
					marginBottom="5"
					marginLeft="5"
					marginRight="5"
					borderStyle="solid"
					borderSize="1"
					borderRadius="10"
					shadow="true"
					shadowColor="0x666666"	
					borderColor="0x666666"
				/>
					<ProjectBoxOverStyle 
					backgroundColor="0x666666"	
					foregroundColor="0xffffff"
				/>
					<ProjectBoxSelectedStyle 
					backgroundColor="0x000000"	
					foregroundColor="0xffffff"	
				/>
				</global>
				<Constant>
					<CostanteDelPannelloDiControllo
					fontFamily="Arial"
					fontSize="11"
					fontWeight="normal"
					foregroundColor="0x444444"
					foregroundAlpha="1"
					align="left"
					marginBottom="0"
					marginRight="2"
					marginTop="0"
				/>
				</Constant>
				<FieldIO>
					<CampoDiInputDelPannelloDiControllo
					backgroundColor="0xFFFFFF"
					backgroundAlpha="1"
					borderColor="0x303030"
					borderSize="1"
					borderStyle="solid"
					fontFamily="Arial"
					fontSize="12"
					height="18"
					foregroundColor="0x303030"
					foregroundAlpha="1"
					align="left"
					marginLeft="3"
					marginRight="3"
				/>
					<TransformRuleEditWindow
					fontFamily="Arial Mono"
					fontSize="13"
				/>
				</FieldIO>
				<PYLabel>
					<TransformItem
					backgroundAlpha="0" />
					<TransformOver
					backgroundAlpha="1"
					backgroundColor="0xAACCFF"	/>
					<TransformSelected
					backgroundAlpha="1"
					backgroundColor="0x66BBFF"	/>			
				</PYLabel>
				<PYNbox>
					<RiquadroDelPannelloDiControllo
						backgroundColor="0xF2F5FA"
						backgroundAlpha="1"
						borderSize="1"
						gradientType="linear"
						gradientAngle="90"
						toColor="0xDEE3F3"
						toColorAlpha="1"
						toColorRatio="128"
						horizontalSpacing="8"
						verticalSpacing="0"
						paddingBottom="0"
						paddingLeft="8"
						paddingRight="8"
						paddingTop="0"
					/>
				</PYNbox>
				<PYNBoxSelectable>
					<TransformBox
					itemStyle="ComposerStyle:TransformItem"
					overStyle="ComposerStyle:TransformItemOver"
					selectedStyle="ComposerStyle:TransformItemSelected"
					paddingRight="14"
					scrollbardimension="13" />
				</PYNBoxSelectable>

				<PYScrollBar>
					<ComposerScrollBar 
						backgroundAlpha="0.6" 
						backgroundColor="0xcccccc" 
						borderColor="0x000011" 
						borderStyle="solid" 
						borderAlpha=".6" 
						borderSize="1" 
						foregroundAlpha="1" 
						foregroundColor="0x000011" 
						toColor="0x00000e" 
						toColorAlpha="1" 
						gradientAngle="90"
						dimension="20" />
					<ScrollContenitoriPannelloDiControllo
						arrowColor="0x666669"
						backgroundAlpha="0"
						backgroundColor="0x00ff00"
						foregroundAlpha="0"
						foregroundColor="0x00aaff"
					/>
				</PYScrollBar>
				<PYButton>
					<SaveRule
						align="center"
						backgroundAlpha="1"
						backgroundColor="0x22E322"
						borderRadius="10"
						gradientAngle="90"
						gradientType="linear"
						toColor="0xABE3AB"
						marginTop="5"
					/>
					<SaveRuleOver
						align="center"
						backgroundAlpha="1"
						backgroundColor="0xE3FFE3"
						borderRadius="10"
						gradientAngle="90"
						gradientType="linear"
						toColor="0xABE3AB"
						marginTop="5"
					/>
					<CancelRule
						align="center"
						backgroundAlpha="1"
						backgroundColor="0xAAAAAA"
						borderRadius="10"
						gradientAngle="90"
						gradientType="linear"
						toColor="0xCCCCCC"
						marginTop="5"
					/>
					<CancelRuleOver
						align="center"
						backgroundAlpha="1"
						backgroundColor="0xDEDEDE"
						borderRadius="10"
						gradientAngle="90"
						gradientType="linear"
						toColor="0xCCCCCC"
						marginTop="5"
					/>
					<DeleteRule
						align="center"
						backgroundAlpha="1"
						backgroundColor="0xE36666"
						borderRadius="10"
						gradientAngle="90"
						gradientType="linear"
						toColor="0xE3ABAB"
						marginTop="5"
					/>
					<DeleteRuleOver
						align="center"
						backgroundAlpha="1"
						backgroundColor="0xE32222"
						borderRadius="10"
						gradientAngle="90"
						gradientType="linear"
						toColor="0xE3ABAB"
						marginTop="5"
					/>
					<PulsantiMenuSuperiore
						align="center"
						backgroundAlpha="0"
						borderAlpha="0"
						borderRadius="0"
						borderStyle="solid"
						borderSize="1"
						fontFamily="Arial"
						fontSize="10"
						foregroundColor="0xffffff"
						foregroundAlpha="1"
						iconColor="0x000000"
						text=" "
						toColorAlpha="0"
						toColorRatio="128"
						width="24"
						height="24"
						normalStyle="ComposerStyle:PulsantiMenuSuperiore"
						overStyle="ComposerStyle:PulsantiMenuSuperioreOver"
						selectedStyle="ComposerStyle:PulsantiMenuSuperioreSelected"
					/>
					<PulsantiMenuSuperioreOver
						backgroundColor="0xFFFFFF"
						backgroundAlpha="0.35"
						borderAlpha="0.25"
						borderColor="0x000000"
						gradientType="linear"
						gradientAngle="90"
						text=" "
						toColor="0xFFFFFF"
						toColorAlpha="0.25"
						toColorRatio="128"
					/>
					<PulsantiMenuSuperioreSelected
						backgroundColor="0xFFFFFF"
						backgroundAlpha="0.25"
						borderAlpha="0.25"
						borderColor="0x000000"
						gradientType="linear"
						gradientAngle="90"
						text=" "
						toColor="0xAAAAAA"
						toColorAlpha="0.35"
						toColorRatio="128"
					/>
					<SpazioPulsantiMenuSuperiore
						align="center"
						backgroundAlpha="0"
						borderAlpha="0"
						borderRadius="0"
						borderStyle="solid"
						borderSize="0"
						text=" "
						width="6"
						height="24"
					/>
				</PYButton>
			</styles>
		</skinProperties>
	</projectSection>
</root>